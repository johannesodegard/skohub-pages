                              skos:inScheme :spn_terminologi ;
                              dct:bibliographicCitation "FFI-rapport 18-02058 kap. 2.2.2"@nb ;
                              skos:altLabel "matematisk sannsynlighet"@nb ;
                              skos:prefLabel "frekvensbasert sannsynlighet"@nb .


###  https://www.johannesodegard.no/terminologi#funksjon
:funksjon a
                   skos:Concept ;
          skos:broader :verdi ;
          skos:inScheme :spn_terminologi ;
          skos:definition "egenskap ved verdi om at et bortfall vil få konsekvenser for leveransen av varer eller tjenester"@nb ;
          skos:note "Merknad: En funksjon må ha både autensitet, integritet og tilgjengelighet for å ikke falle bort."@nb ;
          skos:prefLabel "funksjon"@nb .


###  https://www.johannesodegard.no/terminologi#fysisk_saarbarhet
:fysisk_saarbarhet a
                            skos:Concept ;
                   skos:broader :saarbarhet ;
                   skos:inScheme :spn_terminologi ;

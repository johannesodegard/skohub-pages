                   skos:prefLabel "fysisk sårbarhet"@nb ;
                   skos:related :fysisk_sikkerhet .


###  https://www.johannesodegard.no/terminologi#fysisk_sikkerhet
:fysisk_sikkerhet a
                           skos:Concept ;
                  skos:broader :domene ;
                  skos:inScheme :spn_terminologi ;
                  skos:definition "aktiviteter og tiltak for å redusere sårbarheter knyttet til fysiske installasjoner og objekter"@nb ;
